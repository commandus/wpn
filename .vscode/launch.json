	"version": "0.2.0",
	"configurations": [
		{
			"name": "wpnr",
			"type": "cppdbg",
			"request": "launch",
			"program": "${workspaceFolder}/wpnr",
			"args": ["-c", "22.js"],
			"stopAtEntry": false,
			"cwd": "${workspaceFolder}",
			"environment": [],
			"externalConsole": true,
			"MIMode": "gdb"
		},
		{
			"name": "wpnw",
			"type": "cppdbg",
			"request": "launch",
			"program": "${workspaceFolder}/wpnw",
			"args": ["-v", "-c", "23.js", "22", "-t", "Съешь еще", "-b", "съешь ещё этих мягких французских булок, да выпей чаю."],
			"stopAtEntry": false,
			"cwd": "${workspaceFolder}",
			"environment": [],
			"externalConsole": true,
			"MIMode": "gdb"
		},
		{
			"name": "wpn --id",
			"type": "cppdbg",
			"request": "launch",
			"program": "${workspaceFolder}/wpn",
			"args": ["-c", "22.js", "--id"],
